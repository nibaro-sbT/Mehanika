package forces;

public class Force {
	double startx;
	double starty;
	
	// in m/s^2
	double intezitet;
	float angleH;

	double momentum;
	
	double x;
	double y;
	public Force(double startx, double starty, double inezitet, float angleH,double momentum) {
		super();
		this.startx = startx;
		this.starty = starty;
		this.intezitet = inezitet;
		this.angleH = angleH;
		this.momentum = momentum;
		
		this.x = (double) (startx + doubleezitet * Math.cos(angleH));
		this.y = (double) (starty - doubleezitet * Math.sin(angleH));
		
		
	}
	
	
	
	public Force(double startx, double starty, double x, double y,double momentum) {
		super();
		this.startx = startx;
		this.starty = starty;
		this.x = startx + x;
		this.y = starty - y;
		this.momentum = momentum;
		
		this.intezitet = Math.sqrt((double)((x-startx)*(x-startx) + (y-starty)*(y-starty)));
		
		this.angleH = Math.acos(Math.abs((double)(y-starty)/intezitet));
	}
	
	public void uppdateMomentum() {
		
		
	}

	public double getStartx() {
		return startx;
	}

	public void setStartx(double startx) {
		this.startx = startx;
	}

	public double getStarty() {
		return starty;
	}

	public void setStarty(double starty) {
		this.starty = starty;
	}

	public double getdoubleezitet() {
		return doubleezitet;
	}

	public void setdoubleezitet(double doubleezitet) {
		this.doubleezitet = doubleezitet;
	}

	public double getAngleH() {
		return angleH;
	}

	public void setAngleH(double angleH) {
		this.angleH = angleH;
	}

	public double getX() {
		return x;
	}

	public void setX(double x) {
		this.x = x;
	}

	public double getY() {
		return y;
	}

	public void setY(double y) {
		this.y = y;
	}



	public double getMomentum() {
		return momentum;
	}



	public void setMomentum(double momentum) {
		this.momentum = momentum;
	}



	
}
